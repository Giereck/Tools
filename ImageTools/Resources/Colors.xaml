<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor1">#2D2D30</Color>
    <Color x:Key="BackgroundColor2">#252526</Color>
    <Color x:Key="BackgroundColor3">#1E1E1E</Color>
    <Color x:Key="TextColor">#C7C7C7</Color>
    <Color x:Key="FolderColor">#D8AC6A</Color>
    <Color x:Key="IconColor">#7AC1FF</Color>
    
    <SolidColorBrush x:Key="BackgroundBrush1" Color="{StaticResource BackgroundColor1}" />
    <SolidColorBrush x:Key="BackgroundBrush2" Color="{StaticResource BackgroundColor2}" />
    <SolidColorBrush x:Key="BackgroundBrush3" Color="{StaticResource BackgroundColor3}" />
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}" />
    <SolidColorBrush x:Key="FolderBrush" Color="{StaticResource FolderColor}" />
    <SolidColorBrush x:Key="IconBrush" Color="{StaticResource IconColor}" />
    
</ResourceDictionary>